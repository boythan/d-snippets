{
  "selectComponent": {
    "prefix": "ds",
    "body": [
      "<Select",
      "\tclassName='$1'",
      "\tvalue={$2}",
      "\terror={$3}",
      "\tdataSource={$4}",
      "\tonChange={(valueId)=>{$5}}",
      "/>"
    ],
    "description": "Creates select component in d-component-library"
  },

  "inputComponent": {
    "prefix": "dit",
    "body": [
      "<InputText",
      "\tclassName='$1'",
      "\tvalue={$2}",
      "\terror={$3}",
      "\tname='$4'",
      "\tonChange={(valueId)=>{$5}}",
      "/>"
    ],
    "description": "Creates input component in d-component-library"
  },

  "buttonComponent": {
    "prefix": "dbt",
    "body": [
      "<Button",
      "\tonClick={()=>{$1}}",
      "\tclassName='${2}'",
      ">",
      "\t\t{$3}",
      "</Button>"
    ],
    "description": "Creates button component in d-component-library"
  },

  "checkboxComponent": {
    "prefix": "dch",
    "body": [
      "<Checkbox",
      "\tchecked={$1}",
      "\tonChange={() =>{$2}}",
      "\tlabel={Messages.$3}",
      "\tclassName='$4'",
      "/>"
    ],
    "description": "Creates button component in d-component-library"
  },

  "checkboxGroupComponent": {
    "prefix": "dchgr",
    "body": [
      "<CheckboxGroup",
      "\tnumberOfColumns='${1:3}'",
      "\tdataSource={$2}",
      "\tlabel={Messages.$3}",
      "\tclassName=`{$4}`",
      "\tvalue={$5}",
      "\tonChange={(value) =>{$6}}",
      "/>"
    ],
    "description": "Creates Checkbox group component in d-component-library"
  },
  "radioGroupComponent": {
    "prefix": "dragr",
    "body": [
      "<RadioGroup",
      "\tnumberOfColumns='${1:3}'",
      "\tdataSource={$2}",
      "\tlabel={Messages.$3}",
      "\tclassName=`{$4}`",
      "\tvalue={$5}",
      "\tonChange={(value) =>{$6}}",
      "/>"
    ],
    "description": "Creates Radio group component in d-component-library"
  },

  "progressComponent": {
    "prefix": "dpr",
    "body": [
      "Progress.show(",
      "\t{ method: $1, params: [${2:input}] },",
      "\t(res: any) => {$3}",
      ");"
    ],
    "description": "Creates progress component in d-component-library"
  },

  "tableComponent": {
    "prefix": "dta",
    "body": [
      "<AwesomeTableComponent",
      "\tref={(ref) => {",
      "\t\t${1:tableRef}.current = ref;",
      "\t}}",
      "\tcolumns={columns}",
      "\tsource={${2:source}}",
      "\ttransformer={${3:transformer}}",
      "\tclassName='$4'",
      "/>"
    ],
    "description": "Creates table component in d-component-library"
  },

  "tableColumnComponent": {
    "prefix": "dtaco",
    "body": [
      "const columns: IColumnsProps = [",
      "\t{",
      "\t\ttitle: Messages.${1},",
      "\t\tdataIndex: '${2}',",
      "\t\trender: (${2}) => (${3})",
      "\t},",
      "]"
    ],
    "description": "Creates table column list in table in d-component-library"
  },

  "tableColumnItem": {
    "prefix": "dtacoit",
    "body": [
      "{",
      "\ttitle: Messages.${1},",
      "\tdataIndex: '${2}',",
      "\trender: (${2}) => (${3})",
      "},"
    ],
    "description": "Creates table column object in table in d-component-library"
  },

  "listComponent": {
    "prefix": "dli",
    "body": [
      "<AwesomeListComponent",
      "\tref={(ref) => {",
      "\t\tactivityList.current = ref;",
      "\t}}",
      "\tsource={source}",
      "\ttransformer={transformer}",
      "\renderItem={renderItem}",
      "/>"
    ],
    "description": "Creates list component in d-component-library"
  },

  "modalClassExportDefault": {
    "prefix": "dmoed",
    "body": [
      "import React from 'react'",
      "import { Modal } from 'd-react-components';",
      "",
      "interface I${1:${TM_FILENAME_BASE}} {",
      "\topen: boolean;",
      "\tonClose:()=> void;",
      "}",
      "",
      "function ${1:${TM_FILENAME_BASE}}({open, onClose}: I${TM_FILENAME_BASE}) {",
      "\treturn (",
      "\t\t<Modal open={open} onClose={onClose} title={$2} onSave={()=>{}}>",
      "\t\t\t<div>$0</div>",
      "\t\t</Modal>",
      "\t)",
      "}",
      "",
      "export default ${1:${TM_FILENAME_BASE}}",
      ""
    ],
    "description": "Creates a React Functional Modal Component"
  }
}
