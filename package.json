{
  "name": "d-snippets",
  "displayName": "d-snippets",
  "description": "React's snippet",
  "version": "0.0.26",
  "publisher": "vinhdxk57",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Snippets"
  ],
  "icon": "images/logo.png",
  "activationEvents": [
    "onCommand:extension.dSnippetSearch"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/boythan/d-snippets.git"
  },
  "keywords": [
    "snippets",
    "react",
    "redux",
    "react-native",
    "javascript",
    "typescript"
  ],
  "main": "./out/index.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.dSnippetSearch",
        "title": "D snippet search"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/js-snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/js-snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/ts-snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/ts-snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/d-component-snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/d-component-snippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/d-component-snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/d-component-snippets.json"
      }
    ]
  },
  "scripts": {
    "vs:prepublish": "npm run compile",
    "vs:publish": "vsce package && vsce publish",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint extension --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.62.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "@vscode/test-electron": "^1.6.2"
  }
}
